{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
      {
        "type": "git",
        "url": "https://github.com/droplister/xcp-core.git"
      }
    ],
    "require": {
        "php": "^7.2",
        "andreasindal/laravel-markdown": "3.0",
        "binarytorch/larecipe": "^1.0",
        "bhavyanshu/chatbase-php": "^1.0",
        "curl/curl": "^1.9",
        "cviebrock/eloquent-sluggable": "^6.0",
        "droplister/xcp-core": "dev-feature/add-dispeners",
        "fideloper/proxy": "^4.0",
        "irazasyed/telegram-bot-sdk": "^2.0",
        "laravel/framework": "^6.0",
        "laravel/helpers": "^1.4",
        "laravel/tinker": "^1.0",
        "laravel/horizon": "^1.3", 
        "maatwebsite/excel": "^3.1",
        "megapixel23/belongs-to-one": "^2.0",
        "morrislaptop/laravel-queue-clear": "~1.0",
        "staudenmeir/eloquent-has-many-deep": "^1.1",
        "tightenco/parental": "0.4-alpha"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
